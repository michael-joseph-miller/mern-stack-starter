{
  "name": "mern-stack-starter",
  "description": "MERN stack starter app",
  "version": "1.0.0",
  "author": "Michael Joseph Miller",
  "license": "MIT",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "install": "npm i && cd client/ && npm i",
    "start": "node server.js",
    "build": "cd client/ && npm run build",
    "dev": "concurrently \"nodemon server.js\" \"cd client/ && npm run watch\""
  },
  "keywords": [
    "mern",
    "starter",
    "mongodb",
    "express",
    "react",
    "node"
  ],
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.0.1",
    "@parcel/transformer-sass": "^2.0.1",
    "@parcel/transformer-webmanifest": "^2.0.1",
    "concurrently": "^6.4.0",
    "nodemon": "^2.0.15"
  },
  "engines": {
    "node": "^16.13.1",
    "npm": "^8.3.0"
  }
}
